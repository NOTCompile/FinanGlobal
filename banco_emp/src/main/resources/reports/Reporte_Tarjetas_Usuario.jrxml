    <query language="SQL"><![CDATA[SELECT
    t.id,
    tt.tipo AS tipo_tarjeta,
    t.codigo,
    t.cvc,
    t.fecha_caducidad,
    cb.nombre AS nombre_cuenta,
    cb.n_cuenta AS numero_cuenta,
    cb.saldo AS saldo_cuenta,
    CONCAT(u.nombre, ' ', u.apellidos) AS nombre_completo,
    u.dni_ruc
FROM tarjetas t
JOIN tipo_tarjeta tt ON t.id_tipo = tt.id
JOIN cuenta_bancaria cb ON t.id_cuenta = cb.id
JOIN t_usuario u ON cb.id_usuario = u.id_usuario
WHERE u.id_usuario = $P{idUsuario}
ORDER BY t.fecha_caducidad DESC]]></query>
