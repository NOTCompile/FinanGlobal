    <query language="sql"><![CDATA[SELECT
    t.id,
    tt.tipo AS tipo_tarjeta,
    tt.descripcion AS tipo_descripcion,
    t.codigo,
    t.cvc,
    t.fecha_caducidad,
    cb.nombre AS nombre_cuenta,
    cb.n_cuenta AS numero_cuenta,
    cb.saldo,
    CONCAT(u.nombre, ' ', u.apellidos) AS titular,
    u.dni_ruc,
    u.correo,
    CASE
        WHEN t.fecha_caducidad < CURRENT_DATE THEN 'VENCIDA'
        WHEN t.fecha_caducidad <= CURRENT_DATE + INTERVAL '3 months' THEN 'POR VENCER'
        ELSE 'VIGENTE'
    END AS estado_tarjeta,
    (t.fecha_caducidad - CURRENT_DATE) AS dias_vigencia
FROM tarjetas t
JOIN tipo_tarjeta tt ON t.id_tipo = tt.id
JOIN cuenta_bancaria cb ON t.id_cuenta = cb.id
JOIN t_usuario u ON cb.id_usuario = u.id_usuario
ORDER BY t.fecha_caducidad]]></query>
